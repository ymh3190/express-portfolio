<%- include('base') %>

    <ul>
        <% if (results) { %>
            <% for(const result of results) { %>
                <li>
                    <span>id:</span>
                    <%= result.id %>,
                        <span>name:</span>
                        <%= result.name %>,
                            <span>price:</span>
                            <%= result.price %>
                </li>
                <% } %>
                    <% } else if (result) { %>
                        <li>
                            <span>id:</span>
                            <%= result.id %>,
                                <span>name:</span>
                                <%= result.name %>,
                                    <span>price:</span>
                                    <%= result.price %>
                        </li>
                        <% } %>
    </ul>